{
  "compilerOptions": {
    "composite": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    },
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext", "DOM", "DOM.Iterable"],
    "types": ["node", "vite/client","element-plus/global"],
    "jsxImportSource": "vue",
    "noImplicitThis": true,
    "verbatimModuleSyntax": true,
    // "forceConsistentCasingInFileNames": true,
    "skipLibCheck": false,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "types/env.d.ts",
    "types/*.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "auto-imports.d.ts",
    "components.d.ts",
    "docs/demo/*.vue",
    "docs/demo/*.ts",
  ],
  // ,"node_modules"
  "exclude": ["src/**/__tests__/*","node_modules", "dist"] 
}
